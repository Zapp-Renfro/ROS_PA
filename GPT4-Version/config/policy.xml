<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE policymap [
<!ELEMENT policymap (policy)+>
<!ELEMENT policy EMPTY>
<!ATTLIST policy domain (delegate|coder|path|resource|system) #IMPLIED>
<!ATTLIST policy name CDATA #IMPLIED>
<!ATTLIST policy value CDATA #IMPLIED>
<!ATTLIST policy stealth (True|False) #IMPLIED>
]>
<policymap>
  <policy domain="path" rights="read|write"/>
  <policy domain="coder" rights="read|write"/>
  <policy domain="delegate" rights="read|write"/>
</policymap>
